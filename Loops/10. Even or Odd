public class EvenOdd {

	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int num=sc.nextInt();
		if(isCheck(num)) {
			System.out.println(num+" is Even");
		}
		else {
			System.out.println(num+" is Odd");
		}

	}

	public static boolean isCheck(int num) {
		if(num%2==0) {
			return true;
		}
		return false;
	}
}
