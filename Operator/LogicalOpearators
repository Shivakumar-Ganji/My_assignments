public class LogicalOpearators {

	public static void main(String[] args) {
		int a=11;
		int b=10;
		if(a==10 && b==10) {
			System.out.println("both numbers are equal");
		}
		if(a==10 || b==10) {
			System.out.println("anyone one number is 10");
		}
		if(!(a==10)) {
			System.out.println("a is not equal to 10");
		}
	}
}
